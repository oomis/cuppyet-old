<div class="page-header">
  <%= link_to categories_path, class: 'btn btn-default' do %>
    All Categories
  <% end %>
  <%= link_to edit_category_path(@category), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show category</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @category.name %></dd>

  <dt>Desc:</dt>
  <dd><%= @category.desc %></dd>

</dl>
